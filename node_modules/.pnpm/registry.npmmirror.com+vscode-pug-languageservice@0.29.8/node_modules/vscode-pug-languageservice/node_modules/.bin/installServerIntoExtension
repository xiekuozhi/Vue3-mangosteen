#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/86152/desktop/mangosteen-fe-1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/86152/desktop/mangosteen-fe-1/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+vscode-languageserver@8.0.2/node_modules/vscode-languageserver/bin/installServerIntoExtension" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+vscode-languageserver@8.0.2/node_modules/vscode-languageserver/bin/installServerIntoExtension" "$@"
fi
